    <children>
        <!-- Pulsante per "Carica Dendrogramma da File" -->
        <Button fx:id="loadDendrogramButton" layoutX="180.0" layoutY="150.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="240.0" text="(1) Carica Dendrogramma da File" onAction="#onLoadDendrogramFromFile" />

        <!-- Pulsante per "Apprendi Dendrogramma da Database" -->
        <Button fx:id="learnDendrogramButton" layoutX="180.0" layoutY="220.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="240.0" text="(2) Apprendi Dendrogramma da Database" onAction="#onLearnDendrogramFromDB" />
    </children>
</AnchorPane>
